{
  "extends": "tsconfig/react.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "sourceMap": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "allowJs": true
  },
  "include": ["src", "vite.config.ts", "**/*.ts", "**/*.tsx", ".eslintrc.cjs"]
}
