{
  "dependencies": {
    "@reduxjs/toolkit": "1.8.0",
    "core-hooks": "*",
    "core-next-components": "*",
    "core-ui": "*",
    "shared-types": "*",
    "core-utils": "*",
    "next": "12.1.4",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-redux": "8.0.0-beta.4"
  },
  "devDependencies": {
    "@svgr/webpack": "6.2.1",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.0.0",
    "@types/react": "18.0.0",
    "@types/tailwindcss": "2.2.4",
    "autoprefixer": "10.4.2",
    "jest": "27.5.1",
    "next-sitemap": "1.9.12",
    "next-transpile-modules": "9.0.0",
